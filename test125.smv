	next(matrix[0][1]) := matrix[0][1];
	next(matrix[0][2]) := matrix[0][2];
	next(matrix[0][3]) := matrix[0][3];
	next(matrix[0][4]) := matrix[0][4];
	next(matrix[0][5]) := matrix[0][5];
	next(matrix[0][6]) := matrix[0][6];
	next(matrix[0][7]) := matrix[0][7];

