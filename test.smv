		(man_move=RIGHT) & (man_x = 1) & (man_y = 0) & (matrix[1][1] = BLOCK) & (matrix[1][2] = EMPTY) : EMPTY;
		(man_move=LEFT) & (man_x = 1) & (man_y = 2) & (matrix[1][1] = BLOCK) & (matrix[1][0] = EMPTY) : EMPTY;
		(man_move=DOWN) & (man_x = 0) & (man_y = 1) & (matrix[1][1] = BLOCK) & (matrix[2][1] = EMPTY) : EMPTY;
		(man_move=UP) & (man_x = 2) & (man_y = 1) & (matrix[1][1] = BLOCK) & (matrix[0][1] = EMPTY) : EMPTY;

