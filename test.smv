		(man_move=DOWN) & (man_x = 0) & (man_y = 1) & (matrix[1][1] = BLOCK) & (matrix[2][1] = EMPTY) : BLOCK;
		(man_move=UP) & (man_x = 4) & (man_y = 1) & (matrix[3][1] = BLOCK) & (matrix[2][1] = EMPTY) : BLOCK;
